            <stringProp name="script">import groovy.json.JsonBuilder
import groovy.json.JsonSlurper

log.info(&quot;Start Post Script&quot;);

def response = prev.getResponseDataAsString();

if(response != null &amp;&amp; !(response.isEmpty())){
def json = new JsonSlurper().parseText(response)

def builder = new JsonBuilder(json)
log.info(&quot;Post Script Json &quot;+builder.toPrettyString());
if(builder.toPrettyString().contains(&quot;accountUUID&quot;)){

//log response to file
//logFileName = &quot;D:/workspace_jmeter/Doenda.csv&quot;;
log.info(&quot;Post Script File &quot;+vars.get(&quot;file&quot;));
logFileName = vars.get(&quot;file&quot;)+&quot;.csv&quot;;
BufferedWriter outLog = new BufferedWriter(new FileWriter(logFileName, true));
outLog.write(builder.content.accountUUID);
outLog.close();
}
}
log.info(&quot;Stop Post Script&quot;);

</stringProp>
